  <script>
  const dotBtn = document.querySelector('.dot-btn');
  const dotMenu = document.querySelector('.dot-dropdown');

  dotBtn.addEventListener('click', (e) => {
    e.stopPropagation();
    dotMenu.style.display =
      dotMenu.style.display === 'block' ? 'none' : 'block';
  });

  document.addEventListener('click', () => {
    dotMenu.style.display = 'none';
  });
  </script>
<script>
document.addEventListener("DOMContentLoaded", function () {

  const dotBtn = document.querySelector(".dot-btn");
  const dotMenu = document.querySelector(".dot-dropdown");

  if (!dotBtn || !dotMenu) {
    console.error("3-dot menu elements not found");
    return;
  }

  dotBtn.addEventListener("click", function (e) {
    e.stopPropagation();
    dotMenu.style.display =
      dotMenu.style.display === "block" ? "none" : "block";
  });

  document.addEventListener("click", function () {
    dotMenu.style.display = "none";
  });

});
</script>

</body>
